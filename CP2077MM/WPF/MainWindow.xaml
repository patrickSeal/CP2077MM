<Window x:Name="windowmain" x:Class="CP2077MM.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CP2077MM.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="1000" Width="1800"
        FontFamily="Play"
        ThemeMode="Dark">
    <Grid UseLayoutRounding="True">
        <TabControl x:Name="tbcntrl" TabStripPlacement="Left" Margin="0,60,0,59">
            <TabItem x:Name="tab01" Header="Mods" Background="{x:Null}" FontSize="25" IsSelected="False" VerticalContentAlignment="Stretch" UseLayoutRounding="False"/>
            <TabItem x:Name="tab02" Header="Load Order" Background="{x:Null}" FontSize="25" IsSelected="False">
                <Grid Background="Transparent"/>
            </TabItem>
            <TabItem x:Name="tab03" Header="Install Mod" Background="{x:Null}" FontSize="25" IsSelected="False">
                <Grid Background="Transparent"/>
            </TabItem>
            <TabItem x:Name="tab04" Header="Updates" Background="{x:Null}" FontSize="25" IsSelected="False">
                <Grid Background="Transparent"/>
            </TabItem>
            <TabItem x:Name="tab05" Header="Profile" Background="{x:Null}" FontSize="25" IsSelected="False">
                <Grid Background="Transparent"/>
            </TabItem>
            <TabItem x:Name="tab06" Header="Settings" Background="{x:Null}" FontSize="25" IsSelected="False">
                <Grid Background="Transparent"/>
            </TabItem>
            <TabItem x:Name="tab07" Header="Help" Background="{x:Null}" FontSize="25" IsSelected="False">
                <Grid Background="Transparent"/>
            </TabItem>
        </TabControl>
        <ProgressBar x:Name="pb" HorizontalAlignment="Center" Height="59" Margin="0,925,0,0" VerticalAlignment="Top" Width="1800"/>
    </Grid>
</Window>
